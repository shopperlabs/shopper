{
  "name": "shopper/core",
  "description": "Laravel Shopper e-commerce core functionality",
  "license": "MIT",
  "require": {
    "ext-intl": "*",
    "php": "^8.1|^8.2",
    "filament/support": "^3.2",
    "laravel/prompts": "^0.1.15",
    "livewire/livewire": "^3.4",
    "mckenziearts/blade-untitledui-icons": "^1.3",
    "spatie/laravel-medialibrary": "^11.5",
    "spatie/laravel-permission": "^6.7",
    "staudenmeir/laravel-adjacency-list": "^1.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Shopper\\Core\\": "src/",
      "Shopper\\Core\\Database\\Factories\\": "database/factories",
      "Shopper\\Core\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shopper\\Core\\CoreServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
