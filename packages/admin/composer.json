{
  "name": "shopper/framework",
  "description": "A TALL Laravel e-commerce  admin panel to create and manage online shop.",
  "keywords": ["laravel", "livewire", "admin-panel", "shopper", "e-commerce", "headless", "store", "shop"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/shopperlabs/framework/issues",
    "source": "https://github.com/shopperlabs/framework"
  },
  "require": {
    "php": "^8.1",
    "danharrin/livewire-rate-limiting": "^0.3|^1.0",
    "bacon/bacon-qr-code": "^2.0",
    "blade-ui-kit/blade-heroicons": "^1.2",
    "filament/notifications": "^2.17",
    "illuminate/console": "^9.0|^10.0",
    "illuminate/contracts": "^9.0|^10.0",
    "illuminate/cookie": "^9.0|^10.0",
    "illuminate/database": "^9.0|^10.0",
    "illuminate/http": "^9.0|^10.0",
    "illuminate/routing": "^9.0|^10.0",
    "illuminate/session": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0",
    "illuminate/view": "^9.0|^10.0",
    "jenssegers/agent": "^2.6",
    "laravel/helpers": "^1.4.1",
    "livewire/livewire": "^2.10",
    "maatwebsite/excel": "^3.1",
    "milon/barcode": "^9.0|^10.0",
    "moneyphp/money": "^3.3",
    "pragmarx/google2fa": "^8.0",
    "psr/simple-cache": "^3.0",
    "rappasoft/laravel-livewire-tables": "^2.12",
    "shopper/core": "self.version",
    "shopper/sidebar": "self.version",
    "spatie/laravel-package-tools": "^1.15",
    "staudenmeir/laravel-adjacency-list": "^1.0",
    "stevebauman/location": "^6.3.1",
    "wire-elements/modal": "^1.0.0",
    "wireui/wireui": "^1.17.9"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Shopper\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shopper\\ShopperServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
